{
  "name": "Search Tabs Title",
  "description": "A pop-up window that let you find your wished tab in the current window, by a title or URL",
  "version": "1.0.0",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Search tabs title",
    "default_icon": {
      "16": "icons/icons8-search-16.png",
      "32": "icons/icons8-search-32.png",
      "48": "icons/icons8-search-48.png",
      "60": "icons/icons8-search-60.png",
      "128": "icons/icons8-search-128.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icons8-search-16.png",
    "32": "icons/icons8-search-32.png",
    "48": "icons/icons8-search-48.png",
    "60": "icons/icons8-search-60.png",
    "128": "icons/icons8-search-128.png"
  },
  "permissions": [
    "tabs"
  ]
}